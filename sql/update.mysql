update civicrm_msg_template set msg_text='{ts 1=$contact.display_name}Dear %1{/ts},\n\n{ts}Your recurring contribution has been updated as requested:{/ts}\n\n{ts 1=$amount}This recurring donation will be automatically processed and %1 will be charged every {/ts}{if $recur_frequency_interval eq 1}{ts 1=$recur_frequency_unit}%1{/ts}{else}{ts 1=$recur_frequency_interval 2=$recur_frequency_unit}%1 %2{/ts}s{/if}{if $installments ge 1}{ts count=$installments plural=\' for a total of %count installments.\'} for %count installment.{/ts}{/if} \n\n{ts}If you have questions please contact us.{/ts}' where msg_title = 'Contributions - Recurring Updates' and is_reserved = 0;
